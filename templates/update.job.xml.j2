<flow-definition plugin="workflow-job@2.40">
  <description>Wordpress Latest install</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <definition class="org.jenkinsci.plugins.workflow.cps.CpsFlowDefinition" plugin="workflow-cps@2.87">
    <script>node {
    stage(&apos;Git Clone&apos;) { 
        
        sh &quot;git clone https://c7886025cc265940259fba6b3d60c27f73aece95@github.com/murzilius/j2_AWS_cloud&quot;
        
    }
    stage(&apos;Get Wordpress&apos;) { 
        
        sh &quot;cd j2_AWS_cloud &amp;&amp; curl -o wordpress.tar.gz -fSL &apos;https://wordpress.org/latest.tar.gz&apos;&quot;
    }
    stage(&apos;Unpack&apos;) {
       
        sh  &quot;cd j2_AWS_cloud &amp;&amp; tar -xzf wordpress.tar.gz -C ./ &amp;&amp;  rm wordpress.tar.gz &amp;&amp; mv  ./w
ordpress ./www&quot;
        
    }
    
    stage(&apos;Push to Git&apos;) {
        
         sh &quot;cd j2_AWS_cloud &amp;&amp; git add ./www &amp;&amp; git commit -a -m &apos;Jenkins result&apos; &amp;&amp; gi
t push origin --all&quot;
    }
    
    stage(&apos;Clean UP&apos;) { 
        
        sh &quot;rm -r ./j2_AWS_cloud&quot;
        
    }
}
</script>
    <sandbox>true</sandbox>
  </definition>
  <triggers/>
  <disabled>false</disabled>
</flow-definition>